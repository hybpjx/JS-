<script type="text/javascript">

$(function(){
	showScroll();
	function showScroll(){
		$(window).scroll( function() { 
			var scrollValue=$(window).scrollTop();
			scrollValue > 190 ? $('a[class=scroll]').fadeIn():$('a[class=scroll]').fadeOut();
		} );	
		$('#page_auto_scroll').click(function(){
			$("html,body").animate({scrollTop:0},200);	
		});	
	}
	//设置省份显示
	setProvinceName();
	checkPerson();
	showxzdjg();
});

function setProvinceName() {
	if (js_global_areacode == "620000") {
		//$(".sproinceClass").html("甘肃省");
		//$(".sproinceClassgx").html("甘肃省");
		//$('#contactUsID').attr('href','mailto:gansu_tzxm_service' + '@' + 'ndrc.gov.cn');
		//甘肃省修改底部样式
		$('#forGX').hide();
		$('#foot').css('background',"url('/tzxmspweb/tzxmweb/pages/portal/common/images/footerGS.png')center");
		$('#foot').css('height',"80px"); 
	} else if (js_global_areacode == "340000") {
		$(".sproinceClass").html("安徽省");
		$(".sproinceClassgx").html("安徽省");
		$('#contactUsID').attr('href','mailto:anhui_tzxm_service' + '@' + 'ndrc.gov.cn');
		$(".specialsproince").html("<br>网站备案号：皖ICP备09029153号-4&nbsp;&nbsp;"); 
		$('#foot').attr('style','background:#3f74c0'); 
	} else if (js_global_areacode == "630000") {
		$(".sproinceClass").html("青海省");
		$(".sproinceClassgx").html("青海省");
		$('#contactUsID').attr('href','mailto:qinghai_tzxm_service' + '@' + 'ndrc.gov.cn');
		$(".specialsproince").prepend('联系电话：09' + '71-63' + '139' + '75，09' + '71-6' + '305' + '126');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" rel="noopener noreferrer">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" rel="noopener noreferrer">地方平台友情链接</a><span id = "zan"> | <a href="javascript:void(0)" onclick="openLikesPage()" style="outline: none">本平台评价</a></span><br>');
	} else if (js_global_areacode == "450000") {
		$(".sproinceClass").html("广西壮族自治区经济");
		$(".sproinceClassgx").html("广西壮族自治区");
		$('#contactUsID').attr('href','mailto:guangxi_tzxm_service' + '@' + 'ndrc.gov.cn');
		$(".specialsproince").prepend('联系电话：07' + '71-2' + '270' + '569');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" rel="noopener noreferrer">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" rel="noopener noreferrer">地方平台友情链接</a><span id = "zan"> | <a href="javascript:void(0)" onclick="openLikesPage()" style="outline: none">本平台评价</a></span><br>');
	} else if (js_global_areacode == "320000") {
