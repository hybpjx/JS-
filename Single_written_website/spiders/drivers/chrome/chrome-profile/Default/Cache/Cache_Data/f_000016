		$(".sproinceClassgx").html("江苏省");
		$('#contactUsID').attr('href','mailto:jiangsu_tzxm_service' + '@' + 'ndrc.gov.cn');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" style="color: #FFF;">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" style="color: #FFF;" rel="noopener noreferrer">地方平台友情链接</a><span id = "zan"> | <a href="javascript:void(0)" onclick="openLikesPage()" style="outline: none; color: #FFF;">本平台评价</a></span><br>');
		$('#foot').css('background',"#1598d5 repeat center -50px");
	}else if (js_global_areacode == "540000") {
		$(".sproinceClass").html("西藏自治区");
		$(".sproinceClassgx").html("西藏自治区");
