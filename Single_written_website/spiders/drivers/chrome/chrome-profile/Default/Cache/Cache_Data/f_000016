<script type="text/javascript">	
   function publicInformation(){
   		var publicInformation = "1";
   		if(publicInformation!="1"){
   			document.getElementById("publicInformationForm").submit();
   		}
   }
   function goPage(optType){
    		switch(optType){
				case 'first':
				  $("#pageNo").val(1);
				  break;
				case 'previous':
				  $("#pageNo").val(parseInt($("#pageNo").val())-1);
				  break;
				case 'next':
				  $("#pageNo").val(parseInt($("#pageNo").val())+1);
				  break;
				case 'last':
				  $("#pageNo").val(14259);
				  break;
				default:
				  $("#pageNo").val(1);
    	}
    	$("#publicInformationForm").submit();
    }
	function goToPage(){
	   	var pageNum = $("#pageNum").val();
	   	$("#pageNo").val(pageNum);
	   	$("#publicInformationForm").submit();
	}
	
    function doSerach(){
    	$("#serviceForm").submit();
    }
    //<!--安徽查询条件增加地区和行业 add by luping@neusoft.com 20160624 start-->
	var setting = {
	    data: {
	        simpleData: {
	            enable: true
	        }
	    },
	    callback: {
	        onClick: zTreeOnClick
	    }
	};
	var settingxz = {
	    data: {
	        simpleData: {
	            enable: true
	        }
	    },
	    callback: {
	        onCheck: zxzTreeOnClick             
	    },
	    check: {
	        enable: true,
	        chkboxType : { "Y" : "p", "N" : "s" }
	    }
	};
	$(document).ready(function(){
  		initxzTreeNodes();
  		inithyTreeNodes();
  	});
    function doSerachAH(){
    	$("#serviceFormAH").submit();
    }
    
    //初始化建设地点 
	function initxzTreeNodes(){
		var data = {
	  			areacode : js_global_areacode
	  		};	
		$.ajax({    
	        type:'post',        
	        //url:poratl.web_ctx+'/projectdeclaration.do?method=getxzTreeNodes',    
	        url:poratl.web_ctx+'/projectdeclaration1.do?method=getxzTreeNodes',    
	        data:data,    
	        cache:false,    
	        dataType:'json',    
	        success:function(data){
			     new PopupLayer({trigger:"#zProvinceAddress",popupBlk:"#blk_xztree",closeBtn:"#close_xz"});
			     $.fn.zTree.init($("#xztree"), settingxz, data);
	        }    
		});
	}
	//初始行业树
	function inithyTreeNodes(){
		$.ajax({    
	        type:'post',        
	        //url:poratl.web_ctx+'/projectdeclaration.do?method=gethyTreeNodes',    
	        url:poratl.web_ctx+'/projectdeclaration1.do?method=gethyTreeNodes',    
	        data:{},    
	        cache:false,    
	        dataType:'json',    
	        success:function(data){
			     new PopupLayer({trigger:"#ele_hy",popupBlk:"#blk_hytree",closeBtn:"#close_hy"});
				 $.fn.zTree.init($("#hytree"), setting, data);
	        }    
		});
	}
  	function zTreeOnClick(event, treeId, treeNode) {
            if(treeNode.id.length >0)
		   //if(!treeNode.isParent)
		    {
		        var node = treeNode ;
		        var arr1 = new Array(); 
		        var i =1 ;
		        arr1[0] = node.name;
		   		while (true) {
		    		node = node.getParentNode() ;
		    		if(node==null){
		    			break;
		    		}else{
		    			arr1[i] =  node.name;
		    		}
		    		i++;
		    	}
		    	var name_str = arr1[arr1.length-1];
		    	for(var j=arr1.length-2;j>=0;j--) { 
					name_str = name_str +" - " + arr1[j];
				}
			   // $("#industry").val(treeNode.name);
			    $("#industry").val(name_str);//显示所有父级名称
			   	$("#industryId").val(treeNode.id);
		    }
        }
        var nodeList=[];
        var index_i = 0;
        function searchNode(namekey,treeId){
            nodeList = [];
            index_i=0;
            var keyValue = $('#'+namekey).val();
            if(keyValue){
                var treeObj = $.fn.zTree.getZTreeObj(treeId);
                nodeList = treeObj.getNodesByParamFuzzy("name", keyValue, null);
                if(nodeList&&nodeList.length>0){
                    treeObj.selectNode(nodeList[0]);
                    treeObj.expandNode(nodeList[0], true, true, true);
                }
            }
        }
        
        function searchNextNode(treeId){
            index_i++;
            var treeObj = $.fn.zTree.getZTreeObj(treeId);
            if(index_i<=nodeList.length) {
                treeObj.selectNode(nodeList[index_i]);
                treeObj.expandNode(nodeList[index_i], true, true, true);
            }
        }
       function zxzTreeOnClick(event, treeId, node) {
            
            var codeTemp = '';          //变量,用来拼接选中树的ID
            var areaDetial = '';        //所属地区详细
            var areaDetialCode = '';    //所属地区详细的ID,只存储到所选最后一级
            var level1num = 0;          //记录有多少个已经选的省区(一级节点)
            var level2num = 0;          //记录当前省下选中市/区(二级节点个数)
            var level3num = 0;          //记录当前市下选中区/县(三级节点个数)
            var divide = '';            //所属地区详细的分隔符,随着级别不同变化 如 A省:a市_1县,2县.3县/b市:4县/C市|B省:c市|C省
            var codeTempout = '';       //境外标记
               
            var treeObj = $.fn.zTree.getZTreeObj("xztree"); //取树
            var sNodes = treeObj.getCheckedNodes(true);     //取树的节点数组
            
            if (sNodes.length > 0) {                                        //如果数组长度大于0
                for(var i=0;i<sNodes.length;i++){                           //循环取树节点
                    switch(sNodes[i].level)                                 //判断树绝点级别
                    {
                        case 0:                                             //一级节点
                            level1num++;
                            if(1 == level1num)                              //一级节点选中一次的情况
                            {
                                divide = '';
                                codeTemp = sNodes[i].id;
                                $("#projectAddress").val(sNodes[i].id);     //projectAddress保存为选中节点的ID
                            }else{                                          //一级节点选中一次以上的情况
                                if(2 == level1num)                          //选中2次在整个函数后边,areaDetialCode已经加上','
                                {
                                    areaDetialCode = areaDetialCode +codeTemp;
                                }else{                                      //选中3次在此加上','分隔
                                    areaDetialCode = areaDetialCode + ',' +codeTemp;
                                }
                                divide = '|';                               //省级分隔符
                                codeTemp =  sNodes[i].id;       
                                            
                                if(codeTemp=="000002")                      //选中多个一级节点,如果同时选中境外,就是跨境
                                {
                                    codeTempout = "000002";
                                }                               
                            }
                            level2num = 0;                                  //将当前省下的市/区数量清零
                            break;
                        case 1:
                            level2num++;                                    
                            if(1 == level2num)                              //二级节点选中一次的情况
                            {
                                codeTemp = sNodes[i].id;
                                divide = ':';
                                if('000001' != $("#projectAddress").val()||'000000' != $("#projectAddress").val())//如果不是跨省区和跨境,二级节点选中一次以上的情况
                                {
                                    $("#projectAddress").val(sNodes[i].id); //取节点ID
                                }
                            }else{
                                codeTemp = codeTemp +','+ sNodes[i].id;
                                divide = '/';
                                if('000001' != $("#projectAddress").val()||'000000' != $("#projectAddress").val())//如果不是跨省区和跨境,二级节点选中一次以上的情况
                                {
                                    $("#projectAddress").val(sNodes[i].getParentNode().id); //取父级节点ID
                                }
                            }
                            
                            level3num = 0;
                            break;
                        case 2:
                            level3num++;
                            if(1 == level3num)                              //三级节点选中一次的情况
                            {
                                codeTemp = sNodes[i].id;                    
                                divide = '_';
                                if('000001' != $("#projectAddress").val()||'000000' != $("#projectAddress").val())
                                {
                                    $("#projectAddress").val(sNodes[i].id);
                                }
                                
                            }else{                                          //三级节点选中一次以上的情况
                                codeTemp = codeTemp +','+ sNodes[i].id;
                                divide = ',';
                                if('000001' != $("#projectAddress").val()||'000000' != $("#projectAddress").val())
                                {
                                    $("#projectAddress").val(sNodes[i].getParentNode().id);
                                }
                            }
                            
                            if(1 != level2num)                          //判断二级节点选中情况,如果超过2个,就取一级节点ID
                            {
                                if('000001' != $("#projectAddress").val()||'000000' != $("#projectAddress").val())
                                {
                                    $("#projectAddress").val(sNodes[i].getParentNode().getParentNode().id);
                                }
                            }                           
                            break;
                        default:
                            divide = '|';
                            level2num = 0;
                            break;
                    }
                    areaDetial = areaDetial + divide + sNodes[i].name ;
                }
                if(1 == level1num)
                {
                    areaDetialCode = areaDetialCode +codeTemp;
                    
                }else{
                    areaDetialCode = areaDetialCode + ',' +codeTemp;
                    if(codeTempout=="000002")                       //选中多个一级节点,如果同时选中境外,就是跨境
                    {   
                        $("#projectAddress").val('000001');
                    }else{                                      //否则,就是跨省
                        $("#projectAddress").val('000000');
                    }
                }   
            }
            $("#areaDetial").val(areaDetial);
            $("#areaDetialCode").val(areaDetialCode);
        }
  	//<!--安徽查询条件增加地区和行业 add by luping@neusoft.com 20160624 end-->
  </script>
  
