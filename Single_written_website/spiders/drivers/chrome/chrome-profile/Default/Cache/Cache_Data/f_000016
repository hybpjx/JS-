<script type="text/javascript">

$(function(){
	showScroll();
	function showScroll(){
		$(window).scroll( function() { 
			var scrollValue=$(window).scrollTop();
			scrollValue > 190 ? $('a[class=scroll]').fadeIn():$('a[class=scroll]').fadeOut();
		} );	
		$('#page_auto_scroll').click(function(){
			$("html,body").animate({scrollTop:0},200);	
		});	
	}
	//设置省份显示
	setProvinceName();
	checkPerson();
	showxzdjg();
	
});

function setProvinceName() {
	if (js_global_areacode == "620000") {
		//$(".sproinceClass").html("甘肃省");
		//$(".sproinceClassgx").html("甘肃省");
		//$('#contactUsID').attr('href','mailto:gansu_tzxm_service' + '@' + 'ndrc.gov.cn');
		//甘肃省修改底部样式
		$('#forGX').hide();
		$('#jiuc').show();
		$('#foot').css('background',"url('/tzxmweb/pages/portal/common/images/footerGS.png')center");
		$('#foot').css('height',"80px");
	} else if (js_global_areacode == "340000") {
		$(".sproinceClass").html("安徽省经济");
		$(".sproinceClassgx").html("安徽省");
		$('#contactUsID').attr('href','mailto:anhui_tzxm_service' + '@' + 'ndrc.gov.cn');
		$(".specialsproince").html("<br>网站备案号：皖ICP备09029153号-4&nbsp;&nbsp;"); 
		$('#foot').attr('style','background:#3f74c0');
	} else if (js_global_areacode == "630000") {
		$(".sproinceClass").html("青海省");
		$(".sproinceClassgx").html("青海省");
		$('#contactUsID').attr('href','mailto:qinghai_tzxm_service' + '@' + 'ndrc.gov.cn');
		$(".specialsproince").prepend('联系电话：09' + '71-6' + '313' + '975，09' + '71-6' + '305' + '126');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" rel="noopener noreferrer">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" rel="noopener noreferrer">地方平台友情链接</a><span id = "zan"> | <a href="javascript:void(0)" onclick="openLikesPage()" style="outline: none">本平台评价</a></span><br>');
	} else if (js_global_areacode == "450000") {
		$(".sproinceClass").html("广西壮族自治区经济");
		$(".sproinceClassgx").html("广西壮族自治区");
		$('#contactUsID').attr('href','mailto:guangxi_tzxm_service' + '@' + 'ndrc.gov.cn');
		$(".specialsproince").prepend('联系电话：07' + '71-2' + '270' + '569');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" rel="noopener noreferrer">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" rel="noopener noreferrer">地方平台友情链接</a><span id = "zan"> | <a href="javascript:void(0)" onclick="openLikesPage()" style="outline: none">本平台评价</a></span><br>');
	} else if (js_global_areacode == "320000") {
		$(".sproinceClass").html("江苏省");
		$(".sproinceClassgx").html("江苏省");
		$('#contactUsID').attr('href','mailto:jiangsu_tzxm_service' + '@' + 'ndrc.gov.cn');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" style="color: #FFF;" rel="noopener noreferrer">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" style="color: #FFF;" rel="noopener noreferrer">地方平台友情链接</a><span id = "zan"> | <a href="javascript:void(0)" onclick="openLikesPage()" style="outline: none; color: #FFF;">本平台评价</a></span><br>');
		$('#foot').css('background',"#1598d5 repeat center -50px");
	}else if (js_global_areacode == "540000") {
		$(".sproinceClass").html("西藏自治区");
		$(".sproinceClassgx").html("西藏自治区");
		$('#contactUsID').attr('href','mailto:xizhang_tzxm_service' + '@' + 'ndrc.gov.cn');
		$('#foot').attr('style','line-height: 22px;padding-top: 5px;'); 
		$("#forGX").prepend('联系我们：  180-8990-5913、 187-8719-7335<br>');
		$("#forGX").prepend('<a href="http://www.tzxm.gov.cn/" target="_blank" rel="noopener noreferrer">中央平台友情链接</a> | <a href="http://www.tzxm.gov.cn/all_links.html" target="_blank" rel="noopener noreferrer">地方平台友情链接</a><br>');
	}
}
function AddFavorite(sURL, sTitle)
{
	var isChrome = navigator.userAgent.toLowerCase().indexOf("chrome") != -1;
	//当浏览器为谷歌浏览器时
	if(isChrome == true){
		alert("添加收藏失败，请按ctrl+D手动添加");
		return false;
		}
		//当浏览器不为谷歌浏览器时
		else{
 		window.sidebar.addPanel(sTitle, sURL, "");
 		}	
}
function openLikesPage() {
	var url = "/gotoevaluate.do";
	var iWidth = window.screen.availWidth > 830 ? 830 : window.screen.availWidth * 0.8; //弹出窗口的宽度;
	var iHeight = window.screen.availHeight > 530 ? 530 : window.screen.availHeight * 0.8; //弹出窗口的高度;
	var iTop = (window.screen.availHeight-30-iHeight)/2; //获得窗口的垂直位置;
	var iLeft = (window.screen.availWidth-10-iWidth)/2; //获得窗口的水平位置;
	window.open(url, 'newwindow', 'height=' + iHeight + ',width=' + iWidth + ',top=' + iTop + ',left=' + iLeft 
		+ ',toolbar=no,menubar=no,scrollbars=yes, resizable=no,location=no, status=no');
}

function checkPerson(){
	if("null" != null && "null" != "null"){
		$("#zan").css("display","");
	}else{
		$("#zan").css("display","none");
	}
}

function showxzdjg(){
	if(js_global_areacode == "540000"){
		$('#xizang').css("display","");
	}
}
