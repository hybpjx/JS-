	function openLikesPage() {
		var url = "/tzxmweb/pages/portal/main_page/gotoevaluate.jsp";
		var iWidth = window.screen.availWidth > 830 ? 830 : window.screen.availWidth * 0.8; //弹出窗口的宽度;
		var iHeight = window.screen.availHeight > 530 ? 530 : window.screen.availHeight * 0.8; //弹出窗口的高度;
		var iTop = (window.screen.availHeight-30-iHeight)/2; //获得窗口的垂直位置;
		var iLeft = (window.screen.availWidth-10-iWidth)/2; //获得窗口的水平位置;
		window.open(url, 'newwindow', 'height=' + iHeight + ',width=' + iWidth + ',top=' + iTop + ',left=' + iLeft 
			+ ',toolbar=no,menubar=no,scrollbars=yes, resizable=no,location=no, status=no');
	}
