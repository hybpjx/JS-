<script type="text/javascript">
	var js_global_areacode = "340000";
	$(document).ready(function() {
		if(js_global_areacode=="620000"){
			$("#nav_box").hide();
			$("#gsmenu_title").show();
		}else{
			$("#gsmenu_title").hide();
			$("#nav_box").show();
		}
	
		if(js_global_areacode=="450000"){
			$("#nav_box").attr("class","nav_boxah");
		}else if (js_global_areacode=="630000" || js_global_areacode=="340000"){
			$("#nav_box").attr("class","nav_boxqh");
		}else{
		//$("#nav_7").hide();
		}
		
		if(js_global_areacode=="630000"){//青海导航条
			$("#nav_1").hide();
			$("#nav_2").hide();
			$("#nav_3").hide();
			$("#nav_4").hide();
			$("#nav_5").hide();
			$("#nav_6").hide();
			$("#nav_7").hide();
			$("#nav_8").hide();
			$("#nav_9").hide();
			$("#qhnav_1").show();
			$("#qhnav_3").show();
			$("#qhnav_4").show();
			$("#qhnav_5").show();
			$("#qhnav_6").show();
			$("#qhnav_7").show();
			$("#qhnav_9").show();
			$("#qhnav_10").show();
			$("#qhnav_11").show();
			$("#ahnav_10").hide();
			$("#ahnav_2").hide();
			$("#ahnav_5").hide();
			$("#ahnav_6").hide();
		}else if(js_global_areacode=="340000"){//安徽导航条
			$("#nav_1").hide();
			$("#nav_2").hide();
			$("#nav_3").hide();
			$("#nav_4").hide();
			$("#nav_5").hide();
			$("#nav_6").hide();
			$("#nav_7").hide();
			$("#nav_8").hide();
			$("#nav_9").hide();
			$("#qhnav_10").hide();
			$("#qhnav_11").hide();
			$("#qhnav_1").show();
			$("#qhnav_3").show();
			$("#qhnav_4").show();
			$("#qhnav_5").hide();
			$("#qhnav_6").hide();
			$("#qhnav_7").show();
			$("#qhnav_9").hide();			
			$("#ahnav_10").show();
			$("#ahnav_2").show();
			$("#ahnav_5").show();
			$("#ahnav_6").show();
		}else{//不是青海，将青海相关都隐藏
			$("#nav_1").show();
			$("#nav_2").show();
			$("#nav_3").show();
			$("#nav_4").show();
			$("#nav_5").show();
			$("#nav_6").show();
			$("#nav_7").show();
			$("#nav_8").hide();
			$("#nav_9").hide();
			$("#qhnav_1").hide();
			$("#qhnav_3").hide();
			$("#qhnav_4").hide();
			$("#qhnav_5").hide();
			$("#qhnav_6").hide();
			$("#qhnav_7").hide();
			$("#qhnav_9").hide();
			$("#qhnav_10").hide();
			$("#qhnav_11").hide();
			$("#ahnav_10").hide();
			$("#ahnav_2").hide();
			$("#ahnav_5").hide();
			$("#ahnav_6").hide();
		}
		
		if(js_global_areacode=="320000") {// 江苏，导航条内容调整  change by huang-n at 2017-09-06
			$("#nav_2").hide();
			$("#nav_5").hide();
			$("#nav_6").hide();
			$("#nav_8").show();
		} 
		if(js_global_areacode=="450000"){
			$("#nav_9").show();
		}

		if(js_global_areacode=="540000" || js_global_areacode=="620000"||js_global_areacode=="450000" ){
			$("#nav_7").hide();
		}
	
	
	});
</script>
