			function register()
			{
				if(340000=="620000"){
					window.location.href="http://www.gszwfw.gov.cn/gsjis/front/register/corregister.do";
				}//add by wangwei 2017/10/26 安徽政府服务网对接
		        else if(340000=="340000"){
		            var anhuiappcodenew="9fb3d2182da04bf49bf125811ebd3bd0";
					window.location.href="https://sso.ahzwfw.gov.cn/uccp-server/login?appCode="+anhuiappcodenew+"&service=https%3A%2F%2Fwww.ahzwfw.gov.cn%2Fbog-bsdt%2Ftransition.html";
				}else
				{
					doDestroy1(11);
				}
			}
			//add by wangwei 2017/10/26 安徽政府服务网对接
			function loginin()
			{
				if(340000=="2"){
		            var anhuiappcodenew="9fb3d2182da04bf49bf125811ebd3bd0";
					window.location.href="https://sso.ahzwfw.gov.cn/uccp-server/login?appCode="+anhuiappcodenew+"&service=https%3A%2F%2Fwww.ahzwfw.gov.cn%2Fbog-bsdt%2Ftransition.html";
				}else
				{
					doDestroy1(0);
				}
			}
			//安徽统一认证去掉密码修改
			if(340000== "340000"){
				$("#xgmm").hide();
			}
